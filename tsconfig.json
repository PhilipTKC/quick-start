{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "target": "ES2017",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "sourceMap": true,
    /**/
    "rootDir": ".",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"]
    },
  },
  "include": ["src"],
  "files": ["src/resources.d.ts"]
}
